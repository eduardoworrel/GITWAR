# PostgreSQL
POSTGRES_USER=gitworld
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=gitworld

# S2.dev Streaming (Backend - write)
S2_TOKEN=your_s2_token_here
S2_BASIN=gitworld
S2_STREAM_NAME=game-state-dev

# S2.dev Streaming (Frontend - read-only, optional)
# If set, browser connects directly to S2 without proxy
VITE_S2_READ_TOKEN=your_s2_read_only_token_here
VITE_S2_BASIN=gitworld
VITE_S2_STREAM=game-state-dev

# Clerk Authentication
CLERK_DOMAIN=your_clerk_domain.clerk.accounts.dev
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key

# Redis Cache (optional - uses memory cache as fallback)
Redis__Enabled=true
Redis__ConnectionString=redis:6379

# GitHub API (para fetch de stats)
GITHUB_TOKEN=ghp_your_github_token

# ASP.NET Core
ASPNETCORE_ENVIRONMENT=Development

# DigitalOcean (deploy only)
DIGITALOCEAN_APP_ID=your_app_id_here
